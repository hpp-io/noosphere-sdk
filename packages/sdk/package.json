{
  "name": "@noosphere/sdk",
  "version": "0.2.0-alpha.1",
  "description": "Noosphere SDK - Unified package for decentralized compute agent development",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsup",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "noosphere",
    "blockchain",
    "compute",
    "agent",
    "decentralized",
    "sdk"
  ],
  "author": "Noosphere Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hpp-io/noosphere-sdk.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/hpp-io/noosphere-sdk/issues"
  },
  "homepage": "https://github.com/hpp-io/noosphere-sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@noosphere/agent-core": "0.2.0-alpha.1",
    "@noosphere/contracts": "0.2.0-alpha.1",
    "@noosphere/crypto": "0.2.0-alpha.1",
    "@noosphere/payload": "0.2.0-alpha.1",
    "@noosphere/registry": "0.2.0-alpha.1"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.8"
  }
}
