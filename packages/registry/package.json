{
  "name": "@noosphere/registry",
  "version": "0.2.1-alpha.1",
  "description": "Container and Verifier registry for Noosphere SDK",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "noosphere",
    "registry",
    "container",
    "verifier"
  ],
  "author": "Noosphere Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hpp-io/noosphere-sdk.git",
    "directory": "packages/registry"
  },
  "bugs": {
    "url": "https://github.com/hpp-io/noosphere-sdk/issues"
  },
  "homepage": "https://github.com/hpp-io/noosphere-sdk/tree/main/packages/registry#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
