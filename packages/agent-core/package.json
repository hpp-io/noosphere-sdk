{
  "name": "@noosphere/agent-core",
  "version": "0.2.0-alpha.2",
  "description": "Core modules for Noosphere agent (EventMonitor, ContainerManager, NoosphereAgent)",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --exclude='**/integration/**'",
    "test:watch": "vitest --exclude='**/integration/**'",
    "test:integration": "vitest run test/integration",
    "test:all": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "noosphere",
    "agent",
    "blockchain",
    "ethereum"
  ],
  "author": "Noosphere Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hpp-io/noosphere-sdk.git",
    "directory": "packages/agent-core"
  },
  "bugs": {
    "url": "https://github.com/hpp-io/noosphere-sdk/issues"
  },
  "homepage": "https://github.com/hpp-io/noosphere-sdk/tree/main/packages/agent-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@noosphere/contracts": "0.2.0-alpha.1",
    "@noosphere/crypto": "0.2.0-alpha.1",
    "@noosphere/payload": "0.2.0-alpha.1",
    "@noosphere/registry": "0.2.0-alpha.1",
    "axios": "^1.13.2",
    "dockerode": "^4.0.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.9.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@types/dockerode": "^3.3.23",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
